{
  "id": null,
  "title": "DevOps AI Platform - Overview Dashboard",
  "tags": ["devops-ai-platform", "overview"],
  "style": "dark",
  "timezone": "browser",
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "Platform Health Status",
      "type": "stat",
      "targets": [
        {
          "expr": "up{job=\"devops-ai-platform\"}",
          "legendFormat": "Platform Status"
        }
      ],
      "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {"color": "red", "value": 0},
              {"color": "green", "value": 1}
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Active AI Agents",
      "type": "stat",
      "targets": [
        {
          "expr": "agent_status{status=\"active\"}",
          "legendFormat": "Active Agents"
        }
      ],
      "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0},
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {"color": "red", "value": 0},
              {"color": "yellow", "value": 5},
              {"color": "green", "value": 10}
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "title": "Total Executions Today",
      "type": "stat",
      "targets": [
        {
          "expr": "increase(agent_executions_total[24h])",
          "legendFormat": "Executions"
        }
      ],
      "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0},
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Success Rate",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(agent_executions_total{status=\"success\"}[5m]) / rate(agent_executions_total[5m]) * 100",
          "legendFormat": "Success Rate %"
        }
      ],
      "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {"color": "red", "value": 0},
              {"color": "yellow", "value": 80},
              {"color": "green", "value": 95}
            ]
          }
        }
      }
    },
    {
      "id": 5,
      "title": "Platform CPU Usage",
      "type": "timeseries",
      "targets": [
        {
          "expr": "process_cpu_seconds_total",
          "legendFormat": "CPU Usage"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 6,
      "title": "Platform Memory Usage",
      "type": "timeseries",
      "targets": [
        {
          "expr": "process_resident_memory_bytes",
          "legendFormat": "Memory Usage"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 7,
      "title": "Database Connections",
      "type": "timeseries",
      "targets": [
        {
          "expr": "pg_stat_database_numbackends",
          "legendFormat": "PostgreSQL - {{datname}}"
        },
        {
          "expr": "redis_connected_clients",
          "legendFormat": "Redis Clients"
        },
        {
          "expr": "mongodb_mongod_connections",
          "legendFormat": "MongoDB Connections"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 8,
      "title": "API Response Time",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(http_request_duration_seconds_sum[5m]) / rate(http_request_duration_seconds_count[5m])",
          "legendFormat": "Avg Response Time"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 9,
      "title": "Agent Performance by Type",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(agent_executions_total[5m])",
          "legendFormat": "{{agent_type}}"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 10,
      "title": "Error Rate by Agent",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(agent_errors_total[5m])",
          "legendFormat": "{{agent_type}}"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 11,
      "title": "Telegram Bot Activity",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(telegram_commands_total[5m])",
          "legendFormat": "Commands/sec"
        },
        {
          "expr": "rate(telegram_messages_total[5m])",
          "legendFormat": "Messages/sec"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32},
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 12,
      "title": "Cost Analysis",
      "type": "timeseries",
      "targets": [
        {
          "expr": "aws_cost_current_month",
          "legendFormat": "Current Month Cost"
        },
        {
          "expr": "aws_cost_previous_month",
          "legendFormat": "Previous Month Cost"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32},
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    }
  ]
}
